{
  "name": "@exode-team/router.tsx",
  "version": "0.0.39",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "src/index.ts",
  "scripts": {
    "build": "tsc && ./node_modules/.bin/babel src --out-dir dist --extensions '.ts,.tsx'",
    "win-build": "tsc && babel src --out-dir dist --extensions '.ts,.tsx' && npm version patch && git push && npm publish",
    "win-publish": "node package-scripts.js win-publish",
    "test": "jest",
    "lint": "eslint --fix --ext .ts,.tsx src",
    "lint-check": "eslint --ext .ts,.tsx src",
    "pub": "yarn run lint-check && yarn run test && yarn version --patch && yarn run build && yarn publish --new-version $(node -p -e \"require('./package.json').version\")",
    "fix-for-link": "rm -rf node_modules/react node_modules/react-dom",
    "fix-for-test": "rm -rf node_modules && rm package-lock.json  && yarn install",
    "docs": "./node_modules/.bin/typedoc --externalPattern 'src/index.ts' --exclude **/*.spec.ts* --excludeExternals --excludeNotExported --out docs src/"
  },
  "author": "AYA Studio",
  "description": "router.tsx â€” drop-in versatile replacement package for the basic React router. An advanced React router built on TypeScript.",
